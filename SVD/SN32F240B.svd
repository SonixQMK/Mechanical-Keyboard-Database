                </enumeratedValue>
                <enumeratedValue>
                  <name>0011b</name>
                  <description>HCLK is more than 12MHz, and less than or equal to 24MHz</description>
                  <value>3</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>0101b</name>
                  <description>HCLK is more than 24MHz</description>
                  <value>5</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>FMCKEY</name>
              <description>FMC verify key</description>
              <bitRange>[31:16]</bitRange>
              <access>write-only</access>
            </field>
     	  </fields>
        </register>
        
        <!-- FLASH_STATUS: Flash Status Register -->
        <register>
          <name>STATUS</name>
          <description>Offset:0x04 Flash Status Register</description>
          <addressOffset>0x04</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x00000005</resetMask>

          <fields>
            <field>
              <name>BUSY</name>
