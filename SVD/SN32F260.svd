          <resetMask>0x0000000F</resetMask>

          <fields>
            <field>
              <name>LPMODE</name>
              <description>Flash Low Power mode selection bit</description>
              <bitRange>[3:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>HCLK is less than 24MHz</description>
                  <value>0</value>
                </enumeratedValue>

                <enumeratedValue>
                  <name>5</name>
                  <description>HCLK is more than or equal to  24MHz</description>
                  <value>5</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
     	  </fields>
        </register>
        
        <!-- FLASH_STATUS: Flash Status Register -->
        <register>
          <name>STATUS</name>
          <description>Offset:0x04 Flash Status Register</description>
          <addressOffset>0x04</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x00000007</resetMask>

          <fields>
            <field>
              <name>BUSY</name>
              <description>Busy flag</description>
              <bitRange>[0:0]</bitRange>
              <access>read-only</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Idle</name>
                  <description>FMC is idle</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Busy</name>
                  <description>Flash operation is in process</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>ERR</name>
              <description>Erase/Error flag</description>
              <bitRange>[2:2]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>No error</name>
                  <description>No error</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Error</name>
                  <description>The address is illegal or over page boundary</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
     	  </fields>
        </register>

        <!-- FLASH_CTRL: Flash Control Register -->
        <register>
          <name>CTRL</name>
          <description>Offset:0x08 Flash Control Register</description>
          <addressOffset>0x08</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0x000000C7</resetMask>

          <fields>
            <field>
              <name>PG</name>
              <description>Flash program mode chosen bit</description>
              <bitRange>[0:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>Disable Flash program mode</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>Enable Flash program mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>PER</name>
              <description>Page erase mode chosen bit</description>
              <bitRange>[1:1]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>Disable page erase mode</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>Enable page erase mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>MER</name>
              <description>Mass erase mode chosen bit</description>
              <bitRange>[2:2]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>Disable masse erase mode</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>Enable mass erase mode</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>START</name>
              <description>Start erase/program operation</description>
              <bitRange>[6:6]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>0</name>
                  <description>Stop/finish operation</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>1</name>
                  <description>Start erase/program operation</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <field>
              <name>CHK</name>
              <description>Checksum calculation chosen</description>
              <bitRange>[7:7]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <enumeratedValue>
                  <name>Disable</name>
                  <description>Disable</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Enable</name>
                  <description>Trigger checksum calculation</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
     	  </fields>
        </register>

        <!-- FLASH_DATA: Flash Data Register -->
        <register>
          <name>DATA</name>
          <description>Offset:0x0C Flash Data Register</description>
          <addressOffset>0x0C</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>

        <!-- FLASH_ADDR: Flash Address Register -->
        <register>
          <name>ADDR</name>
          <description>Offset:0x10 Flash Address Register</description>
          <addressOffset>0x10</addressOffset>
          <size>32</size>
          <access>read-write</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
        </register>

        <!-- FLASH_CHKSUM: Flash Checksum Register -->
        <register>
          <name>CHKSUM</name>
          <description>Offset:0x14 Flash Checksum Register</description>
          <addressOffset>0x14</addressOffset>
          <size>32</size>
          <access>read-only</access>
          <resetValue>0x00000000</resetValue>
          <resetMask>0xFFFFFFFF</resetMask>
          
          <fields>
            <field>
              <name>UserROM</name>
              <description>Checksum of User ROM</description>
              <bitRange>[15:0]</bitRange>
              <access>read-only</access>
            </field>
            <field>
              <name>BootROM</name>
              <description>Checksum of Boot ROM</description>
              <bitRange>[31:16]</bitRange>
              <access>read-only</access>
            </field>
     	  </fields>
        </register>
      </registers>
    </peripheral>   
	
